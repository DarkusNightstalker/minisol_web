	<queryString>
		<![CDATA[SELECT
p.id as id_product,
p.barcode,
p.name,
c.id as id_company,
c.name||' '||c.city||' ('||c.address||')' as company,
s.quantity,
uom.abbr,
(SELECT string_agg(s.description, ', ') FROM product_has_seller phs join seller s on phs.id_seller = s.id WHERE phs.id_product = p.id) AS sellers,
(SELECT string_agg(COALESCE(a.other,a.name), ', ') FROM product_has_seller phs join seller s on phs.id_seller = s.id join actor a on s.id_supplier = a.id WHERE phs.id_product = p.id) AS suppliers
from stock s
right join product p on s.id_product = p.id
left join company c on c.id = s.id_company
join uom uom on uom.id = p.id_uom
WHERE $X{IN, c.id, id_companies} AND p.active = true
ORDER BY p.id]]>
	</queryString>
