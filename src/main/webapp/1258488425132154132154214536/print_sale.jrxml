	<queryString>
		<![CDATA[select
p.name as product_name,
sd.quantity,
uom.abbr as uom_abbr,
sd.unit_price,
sd.subtotal
from sale_detail sd
join product p
on sd.id_product = p.id
join uom uom
on uom.id = sd.id_uom
WHERE sd.id_sale = $P{id_sale};]]>
	</queryString>
