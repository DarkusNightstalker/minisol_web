		<![CDATA[select
p.barcode,
p.name,
st.code as st_code,
st.name as st_name,
uom.code as uom_code,
uom.name as uom_name
from product p
join stock_type st on p.id_stock_type = st.id
join uom on p.id_uom = uom.id
WHERE p.barcode LIKE $P{barcode_product}]]>
	</queryString>
