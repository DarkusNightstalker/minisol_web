	<queryString>
		<![CDATA[select
p.barcode,
ismd.quantity,
uom.abbr as uom_abbr,
ismd.product_name as product_name,
ismd.weight,
uom_w.abbr as uom_w
from ism_detail ismd
join uom uom
on  uom.id = ismd.id_uom
join product p
on p.id = ismd.id_product
left join uom uom_w
on  uom_w.id = ismd.id_weight_uom
WHERE ismd.id_ism = $P{id_ism};]]>
	</queryString>
